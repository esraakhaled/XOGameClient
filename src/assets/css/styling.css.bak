.title{-fx-box-shadow: 45px;
-fx-text-decoration-style: wavy;
-fx-color:#2e423e;}
.play_online, .play_local , .play_computer{
-fx-padding:4px;
-fx-background-radius: 10px;
-fx-font-size:18px;
-fx-font-weight:700;
-fx-color:#2e423e;
-fx-box-shadow: 45px;
}
.screen{
	-Fx-background:rgba(0, 128, 0, 0.1);
	-fx-background-position: center;
	-fx-background-size: cover;
}
.text_field{
	-fx-padding:4px;
	-fx-background-radius: 10px;
	-fx-border-color:#2e423e;
}
